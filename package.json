{
  "name": "starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "yarn run lingui:compile && react-scripts start",
    "build": "yarn run lingui:compile && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lingui:add-locale": "lingui add-locale",
    "lingui:extract": "lingui extract",
    "lingui:compile": "lingui compile",
    "firebase:deploy": "firebase deploy"
  },
  "dependencies": {
    "@lingui/react": "2.8.3",
    "@material-ui/core": "4.5.2",
    "@material-ui/icons": "4.5.1",
    "@types/styled-components": "4.1.20",
    "firebase": "7.2.3",
    "formik": "2.0.3",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-redux": "7.1.1",
    "react-router-dom": "5.1.2",
    "react-scripts": "3.2.0",
    "redux-devtools-extension": "2.13.8",
    "redux-saga": "1.1.1",
    "redux-starter-kit": "1.0.1",
    "styled-components": "4.4.1"
  },
  "devDependencies": {
    "@babel/core": "7.6.4",
    "@commitlint/cli": "8.2.0",
    "@commitlint/config-conventional": "8.2.0",
    "@lingui/cli": "2.8.3",
    "@lingui/macro": "2.8.3",
    "@types/firebase": "3.2.1",
    "@types/lingui__react": "2.8.1",
    "@types/node": "12.12.5",
    "@types/react": "16.9.11",
    "@types/react-dom": "16.9.3",
    "@types/react-redux": "7.1.5",
    "@types/react-router-dom": "5.1.2",
    "babel-core": "7.0.0-bridge.0",
    "eslint-config-prettier": "6.5.0",
    "eslint-plugin-prettier": "3.1.1",
    "firebase-tools": "7.6.2",
    "husky": "3.0.9",
    "lint-staged": "9.4.2",
    "prettier": "1.18.2",
    "typescript": "3.6.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "import/order": [
        2,
        {
          "newlines-between": "always"
        }
      ],
      "react/jsx-sort-props": 2
    }
  },
  "prettier": {
    "printWidth": 85,
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.+(js|jsx)": [
      "eslint --fix",
      "git add"
    ],
    "*.+(json|css|md)": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
